#!/usr/bin/env python

# -*- coding: utf-8 -*-

import os

import sys

import scapy

class bcolors:

    HEADER = '\033[95m'

    OKBLUE = '\033[94m'

    OKGREEN = '\033[92m'

    WARNING = '\033[93m'

    FAIL = '\033[91m'

    ENDC = '\033[0m'

    BOLD = '\033[1m'

    UNDERLINE = '\033[4m'

sys.stdout.write("\x1b]2;|:.orinshi.:|:.users:1.:|:.0.3-Beta.:|\x07")

bots= ("0")

banner= ("Enter Your Target")

def bannana():

    os.system ('clear')

    print ("\033[96m  ▄▀▀▀▀▄   ▄▀▀▄▀▀▀▄  ▄▀▀█▀▄    ▄▀▀▄ ▀▄  ▄▀▀▀▀▄  ▄▀▀▄ ▄▄   ▄▀▀█▀▄   ")

